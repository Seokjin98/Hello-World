public class Hello {
  public static void main(String[] args) {
    System.out.println("Hello, world!");
    System.out.println("Amazing weather outside!");
    System.out.println("This is the third line!");
    System.out.println("How do you do?");
    System.out.println("It rained earlier today");
    System.out.println("I hope tomorrow is going to be sunny");
  }
}
